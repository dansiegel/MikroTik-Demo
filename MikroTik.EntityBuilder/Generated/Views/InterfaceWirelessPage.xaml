            <Label Text="HtAmsduThreshold" FlexLayout.Basis="50%" />
            <Label Text="{Binding HtAmsduThreshold}" FlexLayout.Basis="50%" />
            <Label Text="HtBasicMcs" FlexLayout.Basis="50%" />
